    "dataSource": "db",
    "public": true
  },
  "inventory": {
    "dataSource": "db",
    "public": true
  },
  "company": {
    "dataSource": "db",
    "public": true
  },
  "AuthorizationCode": {
    "dataSource": "db",
    "public": true
  }

