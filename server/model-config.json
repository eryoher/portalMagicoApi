      "remoting": {
        "sharedMethods": {          
          "searchUsers":true
        }
      }
    }
  },
  "promotions": {
    "dataSource": "db",
    "public": true
  },
  "assets": {
    "dataSource": "db",
    "public": true
  },
  "Image": {
    "dataSource": "images",
    "public": true
  },
