{
  "name": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "email": {
      "type": {
        "required": false
      }
    },
    "username": {
      "type": "string",
      "required": false
    },
    "name": {
      "type": "string",
      "required": false
    },
    "lastname": {
      "type": "string",
      "required": false
    },
    "city": {
      "type": "string",
      "required": false
    },
    "phone": {
      "type": "string",
      "required": false
    }, 
    "consentWeb":{
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "Role": {
      "type": "belongsTo",
      "model": "Role",
      "foreignKey": ""
    },
    "inventory": {
      "type": "hasMany",
      "model": "inventory",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }    
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
